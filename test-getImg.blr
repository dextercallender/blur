int[][] reader(string s) {
        int[][] as = readGrayscaleImage(s);
        return as;
}

void cool_dither(int[][] a, char[][] cv, int b) {
        int width = len(a);
        int height = len(a[0]);
        int i;
        int j;
        char px;
        int c = b % 10;
        for(i = 0; i < width; i = i + 1) {
                for(j = 0; j < height; j = j + 1) {
                        px = |a[i][j] - c|;
                        cv[i][j] = px;
                }
        }
        for(i = 0; i < width; i = i + 1) {
            for(j = 0; j < height; j = j + 1) {
                print(cv[i][j]);
            }
            println("");
        }
}

int main() {
        char c = intensityToChar(255);
        int[][] a = reader("images/leaf.jpg");
        char[][] cv = canvas("images/leaf.jpg");
        int i = 0;
        int d;
        while(i < 1000) {
            cool_dither(a, cv, i);
            i = i + 1;
        }
}
